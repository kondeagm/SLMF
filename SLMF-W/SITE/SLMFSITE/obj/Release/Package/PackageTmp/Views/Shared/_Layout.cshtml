@{SLMFSITE.Models.SlmfDBEntities dbV = new SLMFSITE.Models.SlmfDBEntities();}
@{ var bdVDisciplinas = dbV.Disciplina.Where(s => s.ID > 0);}
@{ var bdVDisciplinaPrincipal = bdVDisciplinas.Where(s => s.Visible == true).First();}
@{ var vLink = "";}
@{string vActiveSeccion = ""; }
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>@ViewBag.Title</title>
    <meta name="robots" content="noindex, nofollow">
    <meta name="robots" content="noarchive">
    <meta name="viewport" content="width=device-width">
    @RenderSection("MetaTags", required: false)
    @{var vLinkIcon = Convert.ToString(Session["AppAssetsURL"]) + "favicon.png";}
    <link rel="icon" type="image/png" href="@vLinkIcon">
    <link rel="stylesheet" type="text/css" href="http://cloud.typography.com/6766072/778164/css/fonts.css">
    <link rel="stylesheet" type="text/css" href="/Content/css/layout.css">
    <link rel="stylesheet" type="text/css" href="/Content/Site.css">
    @Scripts.Render("~/script-files/global")
    @RenderSection("LocalCss", required: false)
</head>
<body class="@ViewBag.BodyClass">
    <header id="header-main">
        <div class="header-main-content">
            <nav id="show-disciplines" class="hover-disciplines submenu submenu-hidden">
                <ul>
                    @foreach (var item in bdVDisciplinas)
                    {
                        <li>
                            @{vLink = Convert.ToString(Session["AppURL"]) + item.Nombre.Replace(" ", "-").ToLower();}
                            @{string vStatus = "inactive";
                            if (item.Visible == true) { vStatus = ""; } else { vLink = "#"; }
                            }
                            @{string vActiveDiscipline = "";
                            if (Convert.ToString(Session["Disciplina"]).ToLower() == item.Nombre.Replace(" ", "-").ToLower()) { vActiveDiscipline = "active"; }
                            }
                            @{var vImage = Convert.ToString(Session["ImageAssetsURL"]) + item.SiglasImage;}
                            <a href="@vLink" class="@vStatus @vActiveDiscipline outline outline-@item.Siglas.ToLower()">
                                <span class="soon-link"> SOON </span>
                                @Html.Raw(System.Web.HttpUtility.HtmlDecode(item.LogoSVG))
                                <img src="@vImage" alt="@item.Siglas.ToLower()" class="to-show">
                                <span class="line line-top"></span><span class="line line-bottom"></span>
                            </a>
                        </li>
                    }
                </ul>
            </nav>
            @{var vLinkLogo = Convert.ToString(Session["AppAssetsURL"]) + "slmf.svg";}
            <a id="logo" href="@Convert.ToString(Session["AppURL"])" class="hover-disciplines logo">
                <img src="@vLinkLogo" alt="SLMF">
            </a>
            <nav class="menu-left-1">
                <ul class="menu-left-1">
                    @{vActiveSeccion = ""; }
                    @{if (Convert.ToString(Session["SeccionActiva"]).ToLower() == "about") { vActiveSeccion = "active"; } }
