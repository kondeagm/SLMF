                    @{vActiveSeccion = ""; }
                    @{if (Convert.ToString(Session["SeccionActiva"]).ToLower() == "slmf-team") { vActiveSeccion = "active"; } }
                    <li><a href="/slmf-team" class="@vActiveSeccion"><span></span>SLMF TEAM</a></li>
                    <li><a href="https://www.sololosmasfuertes.com/tienda/index.php/" target="_blank"><span></span>TIENDA</a></li>
