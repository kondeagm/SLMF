@model SLMFSITE.Models.Disciplina
@{
    ViewBag.Title = "SLMF - " + @Model.Nombre;
    ViewBag.BodyClass = "bodybuilding-home";
    ViewBag.NavPilarsClass = "";
}
@{var vLink = "";}
@{var vLinkLogo = Convert.ToString(Session["AppAssetsURL"]) + "slmf-logo.jpg";}
@section MetaTags {
    <meta name="description" content="">
    <meta property="og:description" content="Nuestro sitio está en construcción, y no sólo por un cambio de imagen: te ofreceremos programas de entrenamiento, dietas y todo lo necesario para que también tú estés entre los más fuertes.">
    <meta property="og:image" content="@vLinkLogo">
    <meta property="og:url" content="http://sololosmasfuertes.com">
    <meta property="og:title" content="SÓLO LOS MÁS FUERTES">
}
@section LocalCss {
    @Styles.Render("~/home/css")
}

<div id="slmf-community-tohidde" class="full-all">
    <div id="home-discipline" class="home from-loader">
        <div class="allLoader">
            <div class="hidden">
                @{var vDisciplinaBackground = Convert.ToString(Session["ImageAssetsURL"]) + Model.FileImage;}
                <div id="home-discipline-bg" class="bg alpha-0">
                    <img src="@vDisciplinaBackground" data-size-mode="contain">
                </div>
                <div class="home-content">
                    <header>
                        <div class="header-content @Model.Siglas.ToLower()">
                            <div class="top">
                                <div class="diagonal d1">
                                    <div class="inner">@Model.Siglas.ToUpper()</div>
                                </div>
                                <div class="diagonal d2">
                                    <div class="inner">@Model.Siglas.ToUpper()</div>
                                </div>
                                <div class="diagonal d3">
                                    <div class="inner">@Model.Siglas.ToUpper()</div>
                                </div>
                                <div class="diagonal d4">
                                    <div class="inner">@Model.Siglas.ToUpper()</div>
                                </div>
                            </div>
                            <h1 class="bottom">@Model.Nombre.ToUpper()</h1>
                            <h3 class="bottom">@Model.Slogan.ToUpper()</h3>
                            <span class="description-mask">
                                <p class="description">
                                    @Html.Raw(System.Web.HttpUtility.HtmlDecode(@Model.Proposito.Replace("\r\n", "<br />")))
                                </p>
                            </span>
                        </div>
